            "commandBlocks": [
                {
                    "label": "Markdown:",
                    "commands": [
                        {
                            "label": "<h1>",
                            "type": "rangeCommand",
                            "commandStart": "# ",
                            "commandEnd": "\n"
                        },
                        {
                            "label": "<h2>",
                            "type": "rangeCommand",
